<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat Code Coverage">
        <Configuration>
          <Format>cobertura</Format>
          <ExcludeByFile>
            Tracker.Dotnet.Auth/Program.cs
            Tracker.Dotnet.Auth/Migrations/*.cs
            Tracker.Dotnet.Auth/Interfaces/*.cs
            Tracker.Dotnet.Auth/Models/*.cs
            **/Migrations/*.cs
            **/Program.cs
            **/Startup.cs
            **/Interfaces/*.cs
            **/Models/*.cs
          </ExcludeByFile>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>